<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.yechaoa.materialdesign">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".activity.SwipeRefreshLayoutActivity">
        </activity>
        <activity android:name=".activity.FloatingActionButtonActivity">
        </activity>
        <activity android:name=".activity.ToolbarActivity">
        </activity>
        <activity android:name=".activity.SnackbarActivity">
        </activity>
        <activity android:name=".activity.TabLayoutActivity">
        </activity>
        <activity android:name=".activity.CardViewActivity">
        </activity>
        <activity android:name=".activity.MyDialogActivity">
        </activity>
        <activity
            android:name=".activity.BottomNavigationActivity"
            android:label="@string/title_activity_bottom_navigation">
        </activity>
        <activity
            android:name=".activity.ScrollingActivity"
            android:label="@string/title_activity_scrolling" >
        </activity>
        <activity
            android:name=".activity.TabViewPagerScrollActivity"
            android:label="@string/title_activity_scrolling" >
        </activity>
    </application>

</manifest>